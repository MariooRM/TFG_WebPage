{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": {
        "outputSourceFiles": true
      },
      "path": "node_modules/docdash",
      "theme": "default",
      "navType": "vertical",
      "linenums": true,
      "collapseSymbols": false,
      "inverseNav": false,
      "outputSourceFiles": true,
      "outputSourcePath": true
    },
    "opts": {
      "encoding": "utf8",
      "destination": "./docs/",
      "recurse": true,
      "template": "node_modules/docdash"
    }
  }
  